<?xml version="1.0" encoding="UTF-8"?>


<module rename-to='@gwtModule@'>

  	<!-- Inherit the core Web Toolkit                              -->
  	<inherits name='com.google.gwt.user.User'/>

    <!-- Google GIN: Dependency Injection                          -->
    <inherits name="com.google.gwt.inject.Inject"/>  

    <!-- Configure 'build.gradle' for various target browsers; safari, gecko1_8, ie6, ie8, opera   -->
    <set-property name="user.agent" value="@user.agent@"/>

  	<entry-point class='demo.client.EntryPoint'/>
    <source path='client'/>
    <source path='di'/>

</module>
